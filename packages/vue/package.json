{
  "name": "@rallie/vue",
  "version": "0.16.0",
  "description": "vue binding for rallie",
  "author": "runnan <1608272694@qq.com>",
  "homepage": "https://github.com/ralliejs/rallie#readme",
  "license": "MIT",
  "main": "./dist/hooks.umd.js",
  "module": "./dist/hooks.es.js",
  "types": "./dist/hooks.d.ts",
  "exports": {
    ".": {
      "import": "./dist/hooks.es.js",
      "require": "./dist/hooks.umd.js",
      "types": "./dist/hooks.d.ts"
    },
    "./mixins": {
      "import": "./dist/mixins.es.js",
      "require": "./dist/mixins.umd.js",
      "types": "./dist/mixins.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ralliejs/rallie.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.mjs"
  },
  "bugs": {
    "url": "https://github.com/ralliejs/rallie/issues"
  },
  "peerDependencies": {
    "@rallie/block": ">=0.16.0",
    "vue": ">=2.7.0"
  },
  "devDependencies": {
    "@rallie/block": "workspace:*"
  }
}
